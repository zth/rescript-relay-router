{
  "name": "@rescript-relay-router-example/client-rendering",
  "version": "0.0.16",
  "private": true,
  "engines": {
    "node": ">=16"
  },
  "type": "module",
  "packageManager": "yarn@3.2.1",
  "scripts": {
    "build": "run-s 'build:*'",
    "build:relay": "rescript-relay-compiler",
    "build:rescript": "rescript build -with-deps",
    "build:vite": "vite build --outDir dist/client",
    "preview": "cross-env ENABLE_FILESERVER=true yarn start",
    "router:generate": "rescript-relay-router generate",
    "dev": "run-s build:relay build:rescript && run-p 'dev:*'",
    "dev:vite": "vite",
    "dev:relay": "yarn build:relay --watch",
    "dev:rescript": "yarn build:rescript -w",
    "schema": "npx -y get-graphql-schema http://localhost:4000/graphql > schema.graphql",
    "test": "echo 'No tests implemented for express example' || vitest run",
    "test:watch": "echo 'No tests implemented for express example' || vitest watch"
  },
  "devDependencies": {
    "@remix-run/web-fetch": "^4.1.3",
    "@rescript/react": "0.12.0-alpha.2",
    "@vitejs/plugin-react-refresh": "1.3.6",
    "chalk": "^5.0.1",
    "chokidar": "^3.5.3",
    "cosmiconfig": "^7.0.1",
    "fast-fuzzy": "^1.11.1",
    "fast-glob": "^3.2.11",
    "graphql": "16.2.0",
    "lines-and-columns": "^2.0.3",
    "magic-string": "^0.26.2",
    "meros": "^1.2.0",
    "nodemon": "^2.0.16",
    "npm-run-all": "^4.1.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-relay": "15.0.0",
    "relay-runtime": "15.0.0",
    "rescript": "11.0.0-beta.4",
    "rescript-relay": "3.0.0-alpha.1",
    "rollup": "^2",
    "rollup-plugin-visualizer": "5.5.0",
    "vite": "^4.4.9",
    "vite-plugin-html": "3.2.0"
  },
  "dependencies": {
    "@rescript/core": "^0.5.0",
    "history": "^5.2.0",
    "jsonc-parser": "^3.0.0",
    "rescript-relay-router": "workspace:^",
    "vscode-jsonrpc": "^5.0.1"
  }
}

{
  "name": "rescript-relay-router",
  "jsx": {
    "version": 4,
    "mode": "automatic"
  },
  "version": "0.0.17",
  "sources": [
    {
      "dir": "src",
      "subdirs": true
    },
    {
      "dir": "cli",
      "subdirs": true,
      "type": "dev"
    },
    {
      "dir": "vite-plugins",
      "subdirs": true
    },
    {
      "dir": "test",
      "subdirs": true,
      "type": "dev"
    }
  ],
  "package-specs": [
    {
      "module": "es6",
      "in-source": true
    }
  ],
  "suffix": ".mjs",
  "bs-dependencies": [
    "@rescript/react",
    "rescript-relay",
    "@gabnor/rescript-stdlib"
  ],
  "bs-dev-dependencies": ["rescript-vitest"],
  "bsc-flags": ["-open RescriptStdlib"],
  "js-post-build": {
    "cmd": "./scripts/post-build.sh"
  }
}

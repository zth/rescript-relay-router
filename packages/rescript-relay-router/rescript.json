{
  "name": "rescript-relay-router",
  "jsx": {
    "version": 4,
    "mode": "automatic"
  },
  "uncurried": true,
  "version": "0.0.17",
  "sources": [
    {
      "dir": "src",
      "subdirs": true
    },
    {
      "dir": "cli",
      "subdirs": true,
      "type": "dev"
    },
    {
      "dir": "vite-plugins",
      "subdirs": true
    },
    {
      "dir": "test",
      "subdirs": true,
      "type": "dev"
    }
  ],
  "package-specs": [
    {
      "module": "esmodule",
      "in-source": true
    }
  ],
  "suffix": ".res.mjs",
  "bs-dependencies": ["@rescript/react", "rescript-relay", "@rescript/core"],
  "bs-dev-dependencies": ["@rescript-relay-router/test-utils"],
  "bsc-flags": ["-open RescriptCore"]
}
